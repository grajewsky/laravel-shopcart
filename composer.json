    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laracart\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laracart\\Tests\\": "tests/"
        }
